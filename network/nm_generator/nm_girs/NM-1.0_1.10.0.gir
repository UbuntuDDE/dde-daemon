          <type name="GType" c:type="GType"/>
        </parameter>
        <parameter name="other_type" transfer-ownership="none">
          <doc xml:space="preserve">a connection type to test against @virtual_type</doc>
          <type name="GType" c:type="GType"/>
        </parameter>
