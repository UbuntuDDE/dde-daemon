        <return-value transfer-ownership="none">
          <doc xml:space="preserve">the configured FQDN to send to the DHCP server</doc>
          <type name="utf8" c:type="const char*"/>
        </return-value>
        <parameters>
          <instance-parameter name="setting" transfer-ownership="none">
