      </member>
      <member name="yes"
              value="1"
              c:identifier="NM_SETTING_CONNECTION_AUTOCONNECT_SLAVES_YES"
              glib:nick="yes">
        <doc xml:space="preserve">slaves are brought up when
  master is activated</doc>
      </member>
    </enumeration>
