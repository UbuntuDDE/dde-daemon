      <doc xml:space="preserve">IP Tunneling Settings</doc>
      <constructor name="new"
                   c:identifier="nm_setting_ip_tunnel_new"
                   version="1.2">
        <doc xml:space="preserve">Creates a new #NMSettingIPTunnel object with default values.</doc>
